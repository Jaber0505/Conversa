# CI runtime configuration
ENV_MODE=
DEBUG=
DJANGO_SETTINGS_MODULE=
SECRET_KEY=

# PostgreSQL test database (used by Django in CI)
DJANGO_DB_NAME=
DJANGO_DB_USER=
DJANGO_DB_PASSWORD=
DJANGO_DB_HOST=
DJANGO_DB_PORT=5432

# Django configuration (required for test command to run)
DJANGO_ALLOWED_HOSTS=

# Optional: Render deploy hook for auto-deployment
RENDER_DEPLOY_HOOK_URL=

# Optional: backend URL for frontend build (Vite)
REACT_APP_API_URL=

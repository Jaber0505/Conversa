[run]
branch = True
# Les cibles de couverture (--cov=<app>) sont pass√©es par la CLI
omit =
    */migrations/*
    */tests/*
    */admin.py
    */apps.py
    */__init__.py
    config/*
    manage.py
    wsgi.py
    asgi.py

[report]
skip_covered = True
show_missing = True
exclude_lines =
    pragma: no cover
    if __name__ == .__main__.:
    if settings.DEBUG:
